#: src/gpodder/gui.py:115
msgid "Current maintainer:"
msgstr "Vedlikeholdes av:"

#: src/gpodder/gui.py:117
msgid "Patches, bug reports and donations by:"
msgstr "Fikser, avlusning og donasjoner av:"

#: src/gpodder/gui.py:257
msgid "Username"
msgstr "Brukernavn"

#: src/gpodder/gui.py:270
msgid "Authentication required"
msgstr "Autentisering kreves"

#: src/gpodder/gui.py:275
msgid "New user"
msgstr "Ny bruker"

#: src/gpodder/gui.py:300
msgid "Password"
msgstr "Passord"

#: src/gpodder/gui.py:318
msgid "Select destination"
msgstr "Velg mål"

#: src/gpodder/gui.py:363
msgid "Enter podcast URL..."
msgstr "Angi podcast URL..."

#: src/gpodder/gui.py:392
msgid "Update all"
msgstr "Oppdater alle"

#: src/gpodder/gui.py:398
msgid "Update selected"
