msgid "Username"
msgstr "Nom d'utilisateur"

#: src/gpodder/gui.py:270
msgid "Authentication required"
msgstr "Authentification nécessaire."

#: src/gpodder/gui.py:275
msgid "New user"
msgstr "Nouvel utilisateur"

#: src/gpodder/gui.py:300
msgid "Password"
msgstr "Mot de passe"

